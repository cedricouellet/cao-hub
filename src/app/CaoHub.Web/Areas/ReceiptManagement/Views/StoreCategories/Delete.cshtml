@model StoreCategoryViewModel

@{
    ViewData["Title"] = "Delete";
}

<p>Are you sure you wish to delete the store category '@Model.Name' ?</p>

<form asp-action="ConfirmDelete" asp-route-id="@Model.Id" asp-antiforgery="true" method="post">
    <button type="submit">Confirm</button>
</form>

<a asp-action="Index">Back</a>